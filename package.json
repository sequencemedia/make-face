{
  "name": "@sequencemedia/make-face",
  "version": "1.1.416",
  "description": "Convert font files into CSS @font-face declarations with embedded Base64 data",
  "keywords": [
    "css",
    "font",
    "face",
    "@font-face",
    "encode",
    "base64",
    "embed",
    "otf",
    "ttf",
    "eot",
    "svg",
    "woff",
    "woff2"
  ],
  "main": "./index.mjs",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "sequencemedia@sequencemedia.net",
    "url": "http://sequencemedia.net"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sequencemedia/make-face.git"
  },
  "scripts": {
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "lint": "eslint . --ext .mjs --ext .cjs",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^10.0.1",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "glob-all": "^3.3.1",
    "mime": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@sequencemedia/hooks": "^1.0.405",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "rimraf": "^5.0.0"
  }
}
