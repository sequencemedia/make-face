  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@sequencemedia/hooks": "^1.0.495",
    "eslint": "8.57.0",
    "eslint-config-standard": "^17.1.0",
    "husky": "^9.0.11",
    "rimraf": "^5.0.7"
  }
